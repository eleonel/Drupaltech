; Patches
; Allow for inclusion in installation profiles like ELMS
projects[accessible_content][patch][] = "http://drupal.org/files/accessible_content-libraries_install_profile-1326220-5.patch"

; Fix an issue with content types can't be saved
projects[accessible_content][patch][] = "http://drupal.org/files/ac-form-type-save-1006724-7.patch"

; Allow block positioning in Context to be defined via UI
projects[context][patch][] = "http://drupal.org/files/issues/context-expose-weights.patch"
projects[context][patch][] = "http://drupal.org/files/issues/context-872564-26.patch"

; Weight correctly to avoid conflict with Admin module
projects[context_reaction_theme][patch][] = "http://drupal.org/files/lower_module_weight_to_0-1401510-3.patch"

; Removes focus event so that you can click on things correctly in Date
projects[editablefields][patch][] = "http://drupal.org/files/issues/editablefields-removeFocus.patch"
; Remove edit text from hover state
projects[editablefields][patch][] = "http://drupal.org/files/elms-schedule-remove-edit-text.patch"

; Adds support for node references in Feeds
projects[feeds][patch][] = "http://drupal.org/files/issues/724536-128-feeds-nodereference-mapper.patch"

; Ignores invalid errors thrown by HTTPS based data Feeds
projects[feeds][patch][] = "http://drupal.org/files/elms-feeds-allow-http-secure-by-default.patch"

; Spaces patch to account for og_node_access module
projects[spaces][patch][] = "http://drupal.org/files/issues/spaces.1101738-10.patch"

; Fix incorrect requirement that a folder be writable
projects[timeline][patch][] = "http://drupal.org/files/timeline-writable_fix-1433262_2.patch"
; Disable the history functionality that makes no sense in a CMS
projects[timeline][patch][] = "http://drupal.org/files/timeline-history-disable-553510-10.patch"

; Fix issue with syncing UUIDs in bulk
projects[uuid][patch][] = "http://drupal.org/files/uuidsyncsubmit-1236768-7-d6.patch"

; ELMS specific patch to backport functionality from 7.x branch
projects[wysiwyg_template][patch][] = "http://drupal.org/files/elms-template-location.patch"